{
  "version": 2,
  "public": false,
  "name": "minato",
  "alias": ["minato.now.sh"],
  "env": {
    "PORT": "4466"
  },
  "builds": [
    { "src": "src/index.ts", "use": "@now/node@canary" }
  ],
  "routes": [
    {
      "src": "/src",
      "status": 301,
      "headers": { "Location": "/api" }
    },
    {
      "src": "/",
      "status": 301,
      "headers": { "Location": "/api" }
    }
  ]
}
